#include<iostream>
#include<iomanip>
#include<cmath>
#include<math.h>
using namespace std;
int main()
{
   double X;
  
   cout << "Degrees         Cosine"<<endl;
   for (int i = 0; i <= 180; i = i + 5)
   {
       X = i *M_PI / 180.0;
       cout << left << setw(3) << left << i << "\t\t"<<setw(10) << setprecision(8) << cos(X) << "\t\t"  << endl;

   }

}
